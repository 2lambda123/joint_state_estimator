<?xml version="1.0"?>
<launch>

  <!-- Joint velocity and acceleration estimator -->
  <node name="left_arm_joint_state_estimator" pkg="dumbo_joint_vel_acc_estimator" type="joint_vel_acc_estimator_node" cwd="node" respawn="false" output="screen" >
    <rosparam command="load" file="$(find dumbo_joint_vel_acc_estimator)/config/joint_vel_acc_filter_config.yaml" />
    <remap from="/left_arm_joint_state_estimator/state" to="/left_arm_controller/state"/>
  </node>

</launch>